<h1>Dashboard</h1>

<p>Logged in as <%= current_user.name %></p>
<p><%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
<p><%= link_to 'Edit Profile', edit_user_registration_path %>


<h2>List of Users</h2>
<table border="1">
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= "#{user.city}, #{user.country}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
